{
  "scriptFile": "__init__.py",

  "bindings": [
    {
      "type": "timerTrigger",
      "direction": "in",
      "name": "timer",
      "schedule": "0 * * * * *"
    },
    {
      "type": "queue",
      "direction": "out",
      "name": "$return",
      "queueName": "outputqueue",
      "connection": "AzureWebJobsStorage"
    }
  ]
}